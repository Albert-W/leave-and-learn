{
  "language": "zh-hans",


  "plugins": [
    "github",
    "anchor-navigation-ex",
    "-lunr", 
    "-search", 
    "search-pro",
    "splitter",
    "expandable-chapters",
    "pageview-count",
    "-sharing",
    "sharing-plus",
    "page-footer-ex"

  ],

  "pluginsConfig": {
    "github": {
      "url": "https://github.com/Albert-W/leave-and-learn"
    },

    "anchor-navigation-ex": {
      "isRewritePageTitle": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    },
    "fontsettings": {
      "size": 1
    },
    "pdf": {
      "pageNumbers": true,
      "fontSize": 18,
      "paperSize": "a5"

    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": true,
      "qzone": true,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": true,
      "whatsapp": false,
      "all": [
          "facebook", "google", "twitter",
          "weibo", "qq", "linkedin",
          "qzone", "douban"
      ]
    },
    "page-footer-ex": {
      "copyright": "Copyright &copy [Albert-W](https://github.com/Albert-W/live-and-learn) 2019  ",
      "联系":"![QR](https://github.com/Albert-W/live-and-learn/blob/master/image/qr-green.png)",
      "markdown":true,
      "update_label": "** 文件修订时间： **",
      "update_format": "  YYYY-MM-DD  "
    }


  }
}